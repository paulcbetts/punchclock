<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Punchclock</id>
        <version>$$version$$</version>
        <description>Make sure your asynchronous operations show up to work on time</description>

        <dependencies>
            <dependency id="System.Reactive" version="[3.1.1,4)" />
        </dependencies>
    </metadata>

    <files>
        <file src="bin\Release\netstandard1.0\Punchclock*" target="lib/netstandard1.0" />
        <file src="**\*.cs" target="src" exclude="**\obj\**\*" />
    </files>
</package>
